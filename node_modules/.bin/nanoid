#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/.pnpm/nanoid@3.3.1/node_modules/nanoid/bin/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/.pnpm/nanoid@3.3.1/node_modules/nanoid/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/.pnpm/nanoid@3.3.1/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/.pnpm/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/node_modules:/mnt/c/Users/wenge/Desktop/test/node_modules:/mnt/c/Users/wenge/Desktop/node_modules:/mnt/c/Users/wenge/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/nanoid/bin/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/nanoid/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/.pnpm/nanoid@3.3.1/node_modules/nanoid/bin/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/.pnpm/nanoid@3.3.1/node_modules/nanoid/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/.pnpm/nanoid@3.3.1/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/.pnpm/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/node_modules:/mnt/c/Users/wenge/Desktop/test/node_modules:/mnt/c/Users/wenge/Desktop/node_modules:/mnt/c/Users/wenge/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/nanoid/bin/node_modules:/mnt/c/Users/wenge/Desktop/test/mySource/vue3.2-mySource/node_modules/nanoid/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
fi
