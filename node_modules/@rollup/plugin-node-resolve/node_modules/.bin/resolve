#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/resolve@1.22.0/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/node_modules:/mnt/c/Users/wenge/Desktop/test/node_modules:/mnt/c/Users/wenge/Desktop/node_modules:/mnt/c/Users/wenge/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/@rollup+plugin-node-resolve@13.1.3_rollup@2.67.3/node_modules/@rollup/plugin-node-resolve/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/@rollup+plugin-node-resolve@13.1.3_rollup@2.67.3/node_modules/@rollup/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/@rollup+plugin-node-resolve@13.1.3_rollup@2.67.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/resolve@1.22.0/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/node_modules:/mnt/c/Users/wenge/Desktop/test/node_modules:/mnt/c/Users/wenge/Desktop/node_modules:/mnt/c/Users/wenge/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/@rollup+plugin-node-resolve@13.1.3_rollup@2.67.3/node_modules/@rollup/plugin-node-resolve/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/@rollup+plugin-node-resolve@13.1.3_rollup@2.67.3/node_modules/@rollup/node_modules:/mnt/c/Users/wenge/Desktop/test/vue3zf/demo/node_modules/.pnpm/@rollup+plugin-node-resolve@13.1.3_rollup@2.67.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
fi
